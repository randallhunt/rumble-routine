{
  "manifest_version": 3,
  "name": "Rumble Routine",
  "description": "Rumble scheduler",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.svg"
  },
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "alarms",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://rumble.com/*"
  ]
}
