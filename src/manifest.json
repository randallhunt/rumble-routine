{
  "manifest_version": 3,
  "name": "Rumble Routine",
  "description": "Rumble scheduler",
  "version": "1.0",
  "action": {},
  "background": {
    "service_worker": "worker.js"
  },
  "content_scripts": [{
    "matches": ["https://rumble.com/*"],
    "js": ["content.js"],
    "css": ["content.css"]
  }],
  "minimum_chrome_version": "92",
  "permissions": [
    "activeTab",
    "alarms",
    "notifications",
    "scripting",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "https://rumble.com/*"
  ]
}
