{
  "manifest_version": 3,
  "name": "Rumble Routine",
  "description": "Rumble scheduler",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "default_icon.svg"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://rumble.com/*"
  ]
}
