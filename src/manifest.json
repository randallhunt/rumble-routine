{
  "manifest_version": 3,
  "name": "Rumble Routine",
  "description": "Rumble scheduler",
  "version": "1.0",
  "action": {},
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "notifications",
    "scripting",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "https://rumble.com/*"
  ]
}
